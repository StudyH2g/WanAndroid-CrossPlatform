import 'package:get/get.dart';
import 'package:getx_wan_android/bindings/login_binding.dart';
import 'package:getx_wan_android/pages/home_page.dart';
import 'package:getx_wan_android/pages/login_page.dart';
import 'package:getx_wan_android/pages/tabs_page.dart';

import 'bindings/home_binding.dart';

class Routes {
  static final routes = [
    GetPage(
        name: '/', page: () => const TabsPage()),
    GetPage(
        name: '/home', page: () => const HomePage(), binding: HomeBinding()),
    GetPage(
        name: '/login', page: () => const LoginPage(), binding: LoginBinding()),
  ];
}
